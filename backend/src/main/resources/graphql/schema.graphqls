scalar LocalDateTime

type Comment {
    id: ID!
    author: String
    message: String
    createdAt: LocalDateTime
    approved: Boolean
}

type User {
    id: ID!
    username: String
    email: String
    createdAt: LocalDateTime
}

type UserAuthResponse {
    id: ID!
    username: String
    email: String
    createdAt: LocalDateTime
    token: String!
}